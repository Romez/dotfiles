[core]
	fileMode = false
	editor = nvim
  excludesfile = ~/.gitignore

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%s (%an) %h\" --graph
  type = cat-file -t
  dump = cat-file -p<Paste>
